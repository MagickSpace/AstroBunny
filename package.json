{
  "author": "Hrihorii Ilin",
  "description": "An Accessible Dashboard Theme for Astro including a login page, a dashboard page and several other pages and components with editor.",
  "homepage": "https://astrobunny.devopsick.com",
  "name": "astrobunny",
  "type": "module",
  "version": "0.0.1",
  "workspaces": [
    "dbest-stack/*"
  ],
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "space": "node --max-old-space-size=4096 astro",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "server": "bun ./dist/server/entry.mjs",
    "develop": "bunx --bun astro dev --host",
    "bb": "bunx --bun astro build",
    "up": "bun upgrade --canary"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "@astrojs/node": "^6.0.2",
    "@astrojs/react": "^3.0.2",
    "@astrojs/sitemap": "^3.0.0",
    "@astrojs/solid-js": "^3.0.1",
    "@astrojs/tailwind": "^5.0.0",
    "@astrojs/ts-plugin": "^1.1.3",
    "@fontsource/inter": "^5.0.8",
    "@rollup/plugin-yaml": "^4.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.8.0",
    "@types/node-fetch": "^2.6.6",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "astro": "^3.2.1",
    "graphql": "^16.8.1",
    "graphql-yoga": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.6",
    "solid-js": "^1.7.12",
    "tailwindcss": "^3.3.3",
    "typescript": "latest"
  },
  "devDependencies": {
    "bun-plugin-dts": "^0.2.1",
    "bun-plugin-dts-auto": "^0.2.2",
    "bun-types": "^1.0.3",
    "daisyui": "latest"
  }
}
